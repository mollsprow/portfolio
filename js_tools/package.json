{
  "scripts": {
    "start": "npm run build:dev && onchange \"tailwind.config.js\" \"../*.html\" \"../*.md\" \"../_layouts/*.html\" \"../_layouts/*.md\" \"../_tabs/*.html\" \"../_tabs/*.md\" -- npm run build:dev",
    "build": "postcss ./src/**/*.css -o ../static/css/styles.css --env production",
    "build:dev": "postcss ./src/**/*.css -o ../static/css/styles.css",
    "build:prod": "postcss ./src/**/*.css -o ../static/css/styles.css --env production"
  },
  "author": "Molly Prower <molls12@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.3",
    "autoprefixer": "^10.4.7",
    "material-ui-colors": "^1.0.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-minify": "^1.1.0",
    "tailwindcss": "^3.1.6"
  }
}
